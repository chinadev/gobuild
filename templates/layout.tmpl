<script>
	/* all outside links change to /redirect?url= */
	$("a").click(function(){
		var href = $(this).attr("href");
		if (/^http[s]?:/.test(href)) {
			var newUrl = "/redirect?url="+encodeURIComponent(href);
			$(this).attr("href", newUrl);
		}
	});
